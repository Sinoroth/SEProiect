@model IEnumerable<MVC.Models.NewsFeed>

@{
    ViewBag.Title = "List";
}

<h2>List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.News)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.News)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Date)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.NewsFeedId }) |
            @Html.ActionLink("Details", "Details", new { id=item.NewsFeedId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.NewsFeedId })
        </td>
    </tr>
}

</table>
